<data>
    <xpath
        expr="/form/notebook/page[@id='esale']/notebook/page[@id='configuration']/notebook/page[@id='general']/field[@name='esale_scheduler']"
        position="after">
            <label name="esale_top_menu"/>
            <field name="esale_top_menu"/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id='esale']/notebook/page[@id='configuration']/notebook/page[@id='general']/field[@name='esale_export_sale_filename']"
        position="after">
            <label name="esale_export_product_filename"/>
            <field name="esale_export_product_filename"/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id='esale']/notebook/page[@id='actions']/notebook/page[@id='orders']"
        position="after">
        <page string="Catalog" id="catalog" col="6">
            <button name="export_prices"/>
            <label name="esale_last_prices"/>
            <field name="esale_last_prices"/><newline/>
            <button name="export_products"/>
            <label name="esale_last_products"/>
            <field name="esale_last_products"/><newline/>
            <button name="export_images"/>
            <label name="esale_last_images"/>
            <field name="esale_last_images"/><newline/>
            <button name="export_menus"/>
            <label name="esale_last_menus"/>
            <field name="esale_last_menus"/><newline/>
        </page>
    </xpath>
</data>
